{
	"name": "starter-kit",
	"version": "0.0.1",
	"description": "Starter kit for full-stack JavaScript projects",
	"private": true,
	"scripts": {
		"prebuild": "npm run cleanup",
		"build": "npm run build:server && npm run build:client",
		"build:client": "webpack --config client/webpack/prod.config.js",
		"build:server": "babel server --out-dir dist",
		"cleanup": "rimraf ./dist/*",
		"dev": "concurrently -k -n \"client,server\" \"npm run dev:client\" \"npm run dev:server\"",
		"dev:client": "webpack serve --config client/webpack/dev.config.js",
		"dev:server": "cross-env PORT=3100 nodemon --exitcrash --inspect --delay 500ms --watch server --exec babel-node server/server.js",
		"lint": "npm run lint:eslint && npm run lint:prettier -- --check",
		"preserve": "npm run build",
		"serve": "npm start",
		"start": "node dist/server.js",
		"lint:eslint": "eslint .",
		"lint:fix": "npm run lint:eslint -- --fix && npm run lint:prettier -- --write",
		"lint:prettier": "prettier ."
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/textbook/starter-kit.git"
	},
	"author": "Jonathan Sharpe <mail@jonrshar.pe>",
	"contributors": [],
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/textbook/starter-kit/issues"
	},
	"homepage": "https://github.com/textbook/starter-kit#readme",
	"devDependencies": {
		"@babel/cli": "^7.14.5",
		"@babel/core": "^7.14.6",
		"@babel/node": "^7.14.7",
		"@babel/plugin-proposal-class-properties": "^7.14.5",
		"@babel/plugin-transform-runtime": "^7.14.5",
		"@babel/preset-env": "^7.14.7",
		"@babel/preset-react": "^7.14.5",
		"@codeyourfuture/eslint-config-standard": "^2.1.0",
		"babel-eslint": "^10.1.0",
		"babel-loader": "^8.2.2",
		"concurrently": "^6.2.0",
		"cross-env": "^7.0.3",
		"css-loader": "^5.2.6",
		"eslint": "^7.30.0",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-react": "^7.24.0",
		"eslint-plugin-react-hooks": "^4.2.0",
		"file-loader": "^6.2.0",
		"html-webpack-plugin": "^5.3.2",
		"html-webpack-tags-plugin": "^3.0.1",
		"nodemon": "^2.0.9",
		"prettier": "^2.3.2",
		"prop-types": "^15.7.2",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-router-dom": "^5.3.0",
		"rimraf": "^3.0.2",
		"style-loader": "^3.0.0",
		"wait-on": "^6.0.0",
		"webpack": "^5.43.0",
		"webpack-cli": "^4.7.2",
		"webpack-dev-server": "^3.11.2",
		"webpack-merge": "^5.8.0"
	},
	"dependencies": {
		"@babel/runtime": "^7.14.6",
		"@types/react-transition-group": "^4.4.3",
		"acorn": "^8.5.0",
		"animate.css": "^4.1.1",
		"boostrap": "^2.0.0",
		"bootstrap": "^5.1.1",
		"chart.js": "^3.5.1",
		"crypto-js": "^4.1.1",
		"dotenv": "^10.0.0",
		"email-validator": "^2.0.4",
		"express": "^4.17.1",
		"helmet": "^4.6.0",
		"i": "^0.3.7",
		"jquery": "^3.6.0",
		"jsonwebtoken": "^8.5.1",
		"moment": "^2.29.1",
		"morgan": "^1.10.0",
