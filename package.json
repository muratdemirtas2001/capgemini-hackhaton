		"password-validator": "^5.1.1",
		"pg": "^8.6.0",
		"process": "^0.11.10",
		"react-accessible-accordion": "^4.0.0",
		"react-bar-chart": "^0.1.2",
		"react-bootstrap": "^2.0.0-rc.0",
		"react-chartjs-2": "^3.1.1",
		"react-datetime": "^3.1.1",
		"react-hook-form": "^7.17.2",
		"react-icons": "^4.3.1",
		"react-tabs": "^3.2.2",
